<%- include('../partials/top') %>

<%- include('../partials/loader') %>

<%- include('../partials/header') %>


<!-- ***** Latest Hindi Area Start ***** -->
<section class="poca-latest-epiosodes section-padding-50 mt-70">
  <div class="container">

    <div class="row">
        <div class="col-12 text-center" style="overflow: hidden">
          <script data-cfasync="false" type="text/javascript" src="https://www.geniusdexchange.com/a/display.php?r=3764999"></script>
        </div>
        <!-- Latest songs  -->
        <div class="col-md-12 mt-5">
          <div class="row">
            <!-- Section Heading -->
            <div class="col-12">
              <div class="section-heading ">
                <h4 class="text-capitalize">Search Results</h4>
                <div class="line"></div>
              </div>
            </div>
          </div>
      
          <div class=" poca-portfolio">
            <ul class="list-group">
              <% for(let i=0; i< songs.length ; i++) { %>
              <% if(i === 5) { %>
              <li class="row list-group-item border-0 mt-1 mb-1" style="overflow: hidden;">  
                <script data-cfasync="false" type="text/javascript" src="https://www.geniusdexchange.com/a/display.php?r=3765011"></script>
              </li>
              <% } %>
              <% if(i === 9) { %>
                <li class="row list-group-item border-0 mt-1 mb-1" style="overflow: hidden;">  
                  <script data-cfasync="false" type="text/javascript" src="https://www.geniusdexchange.com/a/display.php?r=3765031"></script>
                </li>
                <% } %>
                
              <li class="row list-group-item border-0 mt-1 mb-1">
                <a href="/lyrics/<%= songs[i]._id %>">
                <div class="col-12  single_gallery_item entre tutor wow fadeInUp" data-wow-delay="0.2s">
                  <div class="poca-music-area style-2 row  flex-wrap">
                    <div class="col-4 h-100 col-sm-3  p-3 ">
                      <img src="/public/img/bg-img/15.jpg" class="h-100 w-100" alt="">
                    </div>
                    <div class="col-8  col-sm-9 p-2">
                      <h6><%= songs[i].Title.length > 20 ? songs[i].Title.slice(0,20): songs[i].Title %></h6>
                      <div class="meta leading-tight text-xs text-grey-dark">
                        <span class="block mt-1 w-full">
                            Singer:
                            <%= songs[i].Singer.split(',')[0] %>
                        </span><br>
                        <span class="block mt-1 w-full">
                            Lyricist:
                            <%= songs[i].Lyricist.split(',')[0] %>
                        </span><br> 
                        <span class="block mt-1 w-full">
                          Language:
                          <%= songs[i].Language %>
                      </span> 
                      </div>
                    </div>
                  </div>
                </div>
              </a>
              </li> 
              <% } %>
            </ul>
      
         
          </div>
        </div>
    </div>

   
  </div>
    
</section>




<%- include('../partials/footer') %>


