<%- include('../partials/top') %>

<%- include('../partials/loader') %>

<%- include('../partials/header') %>


<!-- ***** Latest Hindi Area Start ***** -->
<section class="poca-latest-epiosodes section-padding-50 mt-70">
  <div class="container">

    <div class="row">

        <div class="col-12 text-center" style="overflow:hidden">
          <script data-cfasync="false" type="text/javascript" src="https://www.geniusdexchange.com/a/display.php?r=3765039"></script>
        </div>
        <!-- Latest songs  -->
        <div class="col-md-12 mt-5">
          <div class="row">
            <!-- Section Heading -->
            <div class="col-12">
              <div class="section-heading ">
                <h4 class="text-capitalize">Singers List</h4>
                <div class="line"></div>
              </div>
            </div>
          </div>
      
          <div class=" poca-portfolio">
            <% if(singers.length > 0) { %>
            <ul class="list-group">

              <% for(let i=0; i< singers.length ; i++) { %>
                <% if( i === 5) { %>
                  <li class="row list-group-item border-0 mt-1 mb-1" style="overflow:hidden">
                    <script data-cfasync="false" type="text/javascript" src="https://www.geniusdexchange.com/a/display.php?r=3765043"></script>
                  </li>
                <% } %> 
                <% if( i === 9) { %>
                  <li class="row list-group-item border-0 mt-1 mb-1" style="overflow:hidden">
                    <script data-cfasync="false" type="text/javascript" src="https://www.geniusdexchange.com/a/display.php?r=3764979"></script>
                  </li>
                <% } %> 

              <li class="row list-group-item border-0 mt-1 mb-1">
                <a href="/singer/<%= singers[i].name %>">
                <div class="col-12  single_gallery_item entre tutor wow fadeInUp" data-wow-delay="0.2s">
                  <div class="poca-music-area style-2 row  flex-wrap">
                    <div class="col-4 h-100 col-sm-3  p-3 ">
                      <img src="/public/img/bg-img/23.jpg" class="h-100 w-100" alt="">
                    </div>
                    <div class="col-8  col-sm-9 p-2">
                      <h6><%= singers[i].name %> Songs</h6>
                    </div>
                  </div>
                </div>
              </a>
              </li> 
              <% } %>
            </ul>
            <% }  else {%>
              <div class="w-100 bg-light p-3" style="height: 400px;" >
                <p class="">No Singer Found</p>
              </div>
            <% } %>
         
          </div>
        </div>
    </div>

    <div class="row  mt-4 mr-2">
      <div class="col-12 text-right alpha w-100 pt-2 pb-2"  style="overflow-y: auto;"> 
        
      </div>
      
    </div>
  </div>
    
</section>




<%- include('../partials/footer') %>


