<script src="/public/js/default-assets/active.js"></script>

<script>

  jQuery(() => {

  function printAlphabets(option){
    
    var i = 65;
    var j = 91;
   
    if(option == 'small'){
      i = 97;
      j = 123; 
    }
   
    for(k = i; k < j; k++){
      var str =String.fromCharCode(k);

      let a = `<a class="pl-2 pr-2 pt-1 pb-1 border" href="/singers/${str}">${str}</a>`
      $('.alpha').append(a)
    }

    
   
  }

  printAlphabets()
})
  
</script>
